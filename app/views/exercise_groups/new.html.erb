<h1>New course_exercise_group</h1>

<% form_for(@course_exercise_group) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :course_instance_id %><br />
    <%= f.text_field :course_instance_id %>
  </p>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', course_exercise_groups_path %>