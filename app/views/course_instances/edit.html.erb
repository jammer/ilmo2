<h1>Editing course_instance</h1>

<% form_for(@course_instance, :url => course_course_instance_path(@course_instance.course, @course_instance)) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :course_id %><br />
    <%= f.text_field :course_id %>
  </p>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Back', course_course_instance_path(@course_instance.course, @course_instance) %>

